{
  "manifest_version": 3,
  "name": "DS-160 中文助手",
  "version": "1.1.0",
  "description": "DS-160表单中文翻译助手 - 完整版",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://ceac.state.gov/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/full-translation.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "DS-160 中文助手"
  }
}